{
  "name": "tmux-project-launcher",
  "version": "0.1.0",
  "description": "A CLI for launching projects in tmux",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "tmux-launcher": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --shims",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --shims",
    "test": "vitest",
    "start": "tsx src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@types/node": "^20.19.25",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.5.0",
    "tsx": "^4.16.2",
    "typescript": "^5.4.4",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  }
}
